    <sch:rule id="r-urn-oid-2.16.840.1.113883.10.20.22.1.15-37623-branch-37623-errors-abstract" abstract="true">
      <sch:assert id="a-1098-31911-branch-37623" test="count(cda:time)=1">SHALL contain exactly one [1..1] time (CONF:1098-31911).</sch:assert>
      <sch:assert id="a-1098-31912-branch-37623" test="count(cda:signatureCode)=1">SHALL contain exactly one [1..1] signatureCode (CONF:1098-31912).</sch:assert>
      <sch:assert id="a-1098-31915-branch-37623" test="not(cda:assignedEntity) or cda:assignedEntity[count(cda:id) &gt; 0]">This assignedEntity SHALL contain at least one [1..*] id (CONF:1098-31915).</sch:assert>
    </sch:rule>
    <sch:rule id="r-urn-oid-2.16.840.1.113883.10.20.22.1.15-37623-branch-37623-errors" context="cda:ClinicalDocument[cda:templateId[@root='2.16.840.1.113883.10.20.22.1.15']]/cda:authenticator[cda:assignedEntity[cda:code[@code='ONESELF'][@codeSystem='2.16.840.1.113883.5.111']]]">
