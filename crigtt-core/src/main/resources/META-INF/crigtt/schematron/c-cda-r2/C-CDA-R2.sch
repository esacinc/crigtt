      <sch:assert id="a-1098-31017" test="@classCode='OBS'">SHALL contain exactly one [1..1] @classCode="OBS" (CodeSystem: HL7ActClass 2.16.840.1.113883.5.6) (CONF:1098-31017).</sch:assert>
      <sch:assert id="a-1098-31018" test="@moodCode='EVN'">SHALL contain exactly one [1..1] @moodCode="EVN" (CodeSystem: ActMood 2.16.840.1.113883.5.1001) (CONF:1098-31018).</sch:assert>
      <sch:assert id="a-1098-31437" test="count(cda:statusCode)=1">SHALL contain exactly one [1..1] statusCode (CONF:1098-31437).</sch:assert>
      <sch:assert id="a-1098-31438" test="cda:statusCode[@code='completed']">This statusCode SHALL contain exactly one [1..1] @code="completed" (CodeSystem: ActStatus 2.16.840.1.113883.5.14) (CONF:1098-31438).</sch:assert>
      <sch:assert id="a-1098-31441" test="count(cda:effectiveTime)=1">SHALL contain exactly one [1..1] effectiveTime (CONF:1098-31441).</sch:assert>
      <sch:assert id="a-1098-32630" test="cda:effectiveTime[count(cda:low)=1]">The effectiveTime/low (a.k.a. "onset date") asserts when the condition became biologically active.
This effectiveTime SHALL contain exactly one [1..1] low (CONF:1098-32630).</sch:assert>
    </sch:rule>
    <sch:rule id="r-urn-oid-2.16.840.1.113883.10.20.22.4.127-errors" context="cda:observation[cda:templateId[@root='2.16.840.1.113883.10.20.22.4.127']]">
