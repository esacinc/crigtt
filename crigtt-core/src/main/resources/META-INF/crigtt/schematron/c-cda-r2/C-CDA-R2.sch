      <sch:assert id="a-1098-31657-branch-36716" test="@typeCode='CALLBCK'">SHALL contain exactly one [1..1] @typeCode="CALLBCK" call back contact (CodeSystem: HL7ParticipationType 2.16.840.1.113883.5.90 DYNAMIC) (CONF:1098-31657).</sch:assert>
      <sch:assert id="a-1098-31658-branch-36716" test="count(cda:associatedEntity)=1">SHALL contain exactly one [1..1] associatedEntity (CONF:1098-31658).</sch:assert>
      <sch:assert id="a-1098-31659-branch-36716" test="not(cda:associatedEntity) or cda:associatedEntity[@classCode='ASSIGNED']">This associatedEntity SHALL contain exactly one [1..1] @classCode="ASSIGNED" assigned entity (CodeSystem: RoleClass 2.16.840.1.113883.5.110 DYNAMIC) (CONF:1098-31659).</sch:assert>
      <sch:assert id="a-1098-31660-branch-36716" test="not(cda:associatedEntity) or cda:associatedEntity[count(cda:id) &gt; 0]">This associatedEntity SHALL contain at least one [1..*] id (CONF:1098-31660).</sch:assert>
      <sch:assert id="a-1098-31662-branch-36716" test="not(cda:associatedEntity) or cda:associatedEntity[count(cda:telecom) &gt; 0]">This associatedEntity SHALL contain at least one [1..*] telecom (CONF:1098-31662).</sch:assert>
      <sch:assert id="a-1098-31663-branch-36716" test="not(cda:associatedEntity) or cda:associatedEntity[count(cda:associatedPerson)=1]">This associatedEntity SHALL contain exactly one [1..1] associatedPerson (CONF:1098-31663).</sch:assert>
      <sch:assert id="a-1098-31664-branch-36716" test="not(cda:associatedEntity/cda:associatedPerson) or cda:associatedEntity/cda:associatedPerson[count(cda:name) &gt; 0]">This associatedPerson SHALL contain at least one [1..*] name (CONF:1098-31664).</sch:assert>
    </sch:rule>
    <sch:rule id="r-urn-hl7ii-2.16.840.1.113883.10.20.22.1.4-2014-06-09-36716-branch-36716-errors" context="cda:ClinicalDocument[cda:templateId[@root='2.16.840.1.113883.10.20.22.1.4' and @extension = '2014-06-09']]/cda:participant">
      <sch:extends rule="r-urn-hl7ii-2.16.840.1.113883.10.20.22.1.4-2014-06-09-36716-branch-36716-errors-abstract" />
    </sch:rule>
  </sch:pattern>
  <sch:pattern id="p-urn-oid-2.16.840.1.113883.10.20.22.1.14-errors">
    <sch:rule id="r-urn-oid-2.16.840.1.113883.10.20.22.1.14-errors-abstract" abstract="true">
      <sch:extends rule="r-urn-hl7ii-2.16.840.1.113883.10.20.22.1.1-2014-06-09-errors-abstract" />
      <sch:assert id="a-1098-28947" test="count(cda:templateId[@root='2.16.840.1.113883.10.20.22.1.14'])=1">SHALL contain exactly one [1..1] templateId (CONF:1098-28947) such that it SHALL contain exactly one [1..1] @root="2.16.840.1.113883.10.20.22.1.14" (CONF:1098-28948).</sch:assert>
