    <sch:rule id="r-urn-oid-2.16.840.1.113883.10.20.22.1.15-37608-branch-37608-errors" context="cda:ClinicalDocument[cda:templateId[@root='2.16.840.1.113883.10.20.22.1.15']]/cda:participant[@typeCode='IND']">
